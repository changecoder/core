{
    "compilerOptions": {
      "baseUrl": "./",
      "strictNullChecks": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "jsx": "preserve",
      "noUnusedParameters": true,
      "noUnusedLocals": true,
      "noImplicitAny": true,
      "target": "ESNext",
      "lib": ["DOM", "ESNext"],
      "skipLibCheck": true
    },
    "exclude": ["node_modules"]
}